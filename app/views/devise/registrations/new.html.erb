<div class="container">
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">Sign Up to continue to Sales Analytic</h1>
      <div class="round-box-signup">
        <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name) ) do |f| %>
          <%= devise_error_messages! %>

          <div class="control-group">
            <label class="control-label" for="inputEmail">Email</label>
            <div class="controls">
              <%= f.email_field :email, :autofocus => true, :id => "inputEmail", :placeholder => "Email" %>
            </div>
          </div>
          <div class="control-group">
            <label class="control-label" for="inputEmail">Username</label>
            <div class="controls">
              <%= f.text_field :username, :autofocus => true, :id => "inputEmail", :placeholder => "Username" %>
            </div>
          </div>
          <div class="control-group">
            <label class="control-label" for="inputPassword">Password</label>
            <div class="controls">
              <%= f.password_field :password, :autofocus => true, :id => "inputPassword", :placeholder => "Password" %>
            </div>
            <div class="control-group">
              <label class="control-label" for="inputEmail">Password Confirmation</label>
              <div class="controls">
                <%= f.password_field :password_confirmation, :autofocus => true, :id => "inputPassword", :placeholder => "Password Confirmation" %>
              </div>
            </div>
            <div class="control-group">
              <label class="control-label" for="inputEmail">Approved</label>
              <div class="controls">
                <%= f.text_field :approved, :autofocus => true, :id => "inputEmail", :placeholder => "Approved" %>
              </div>
            </div>
            <div class="control-group">
              <label class="control-label" for="inputEmail">Cabang</label>
              <div class="controls">
                <%= f.text_field :cabang, :autofocus => true, :id => "inputEmail", :placeholder => "Cabang" %>
              </div>
            </div>
            <div class="control-group">
              <label class="control-label" for="inputEmail">Roles</label>
              <div class="controls">
                <%= f.text_field :roles, :autofocus => true, :id => "inputEmail", :placeholder => "Roles" %>
              </div>
            </div>
            <div class="control-group">
              <label class="control-label" for="inputEmail">User Brand</label>
              <div class="controls">
                <%= f.text_field :user_brand, :autofocus => true, :id => "inputEmail", :placeholder => "User brand" %>
              </div>
            </div>
              <div class="control-group">
                <div class="controls"></div>
              </div>
            </div>

          <button type="submit" class="btn btn-lg btn-primary btn-block">Sign up</button>

          <%= render "devise/shared/links" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
