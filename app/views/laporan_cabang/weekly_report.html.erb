         <td>0</td>
       <% else %>
								<% YearlyTarget.get_yearly_target(2, cabang.id, @periode.beginning_of_month.to_date).each do |yearly_target| %>
									<td><%= currency(yearly_target.total.to_i) %></td>
								<% end %>
							<% end %>
							<td>0</td>
				    </tr>
				  <% end %>
				</tbody>
			</table>
		</div>
	</div>
