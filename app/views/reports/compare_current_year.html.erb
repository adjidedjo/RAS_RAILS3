<% if params[:group_by] == 'customer' %>
	<% unless params[:from].nil? %>
  	<% unless params[:from].empty? %>
			<%= render 'compare_current_year_customer', :from => params[:from], :group_by => params[:group_by],
				:to => params[:to], :brand => params[:brand], :type => params[:type], :article => params[:article], :fabric => params[:fabric],
				:size_type => params[:size_type], :size => params[:size], :customer_modern => params[:customer_modern] %>
		<% end %>
	<% end %>
<% end %>

