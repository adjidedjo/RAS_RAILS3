<%= content_tag :div, "", id: "lady_chart", data: {sales: chart_lady} %>

<script type="text/javascript" charset="utf-8">
  $(function () {
    new Morris.Line({
      element: 'lady_chart',
      data: $('#lady_chart').data('sales'), 
      xkey: 'merk',
      ykeys: ['quantity_lady', 'quantity_last_lady'],
      labels: ['Current Month', 'Last Month'],
      parseTime: false,
      resize: true,
      ymin: 0,
      ymax: 300,
      lineColors: ['#0b62a4', '#F07746'],
      gridTextSize: 10
    });
  });
</script>