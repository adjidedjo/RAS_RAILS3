<div class="row-fluid">
  <div class="offset4">
    <div class="span5">
      <!-- block -->
      <div class="block">
        <div class="navbar navbar-inner block-header">
          <div class="muted pull-left">Special Size</div>
          <div class="pull-right"><span class="badge badge-info"></span>

          </div>
        </div>
        <div class="block-content collapse in">
          <%= form_tag reports_size_special_path, :method => 'get' do %>
            <fieldset>
              <%= text_field_tag 'panjang', params[:panjang].nil? ? nil : params[:panjang], :placeholder => "Lebar"  %>
            </fieldset>
            <br />
            <div class="pull-right"><button type="submit" class="btn btn-primary">Next</button></div>
          <% end %>
        </div>
      </div>
      <!-- /block -->
    </div>
  </div>
</div>
